@model PrestaDinero.Core.ValeEntity

<form asp-action="Delete" asp-controller="vale" method="post">

    <div class="modal-header">
        <h5>¿Estas seguro que quieres eliminar?</h5>
    </div>

    <input type="hidden" asp-for="IdVale">
    <div class="card-body">
        <div class="form-group">
            <label asp-for="Cliente.NombreCompleto" class="label-control"></label>
            <input asp-for="Cliente.NombreCompleto" type="date" class="form-control" disabled>

        </div>

        <div class="form-group">
            <label asp-for="Total" class="label-control"></label>
            <input asp-for="Total" class="form-control" disabled>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
        <input type="submit" value="Borrar" class="btn btn-danger swalDefaultSuccess">
    </div>
</form>
